{
    "name": "sevaske/discourse",
    "description": "Discourse laravel package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sevaske\\Discourse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sevaske\\Discourse\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "sevaske"
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-message": "^2.0",
        "psr/http-factory": "^1.1"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.10",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "test": "phpunit --testdox",
        "stan": "phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
